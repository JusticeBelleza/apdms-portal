{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ]
    }
  ],
  "firestore": {
    "database": "(default)",
    "location": "asia-southeast1",
    "rules": "firebase deploy --only firestore:rules",
    "indexes": "firebase deploy --only firestore:indexes"
  }
}
